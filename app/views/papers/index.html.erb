<h1>Listing papers</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Venue</th>
    <th>Year</th>
    <th>Show</th>
  </tr>
  <% @papers.each do |paper| %>
  <tr>
    <td><%= paper.title %></td>
    <td><%= paper.venue %></td>
    <td><%= paper.year %></td>
    <td><%= link_to 'Show', paper_path(paper) %></td>
  </tr>
  <% end %>
</table>
<br>
<%= link_to 'Add paper', new_paper_path %>
